{
  "name": "parser2gis4",
  "version": "4.0.11",
  "description": "Parser 2Gis",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder . --win --ia32 --x64",
    "pub2": "node ../publish/publish2.js"
  },
  "build": {
    "appId": "{9A452D88-9CC1-493A-AA46-8B33FFD4A641}",
    "productName": "Парсер 2Gis",
    "asar": true,
    "win": {
      "target": "nsis"
    },
    "linux": {
      "artifactName": "parser2gis${version}.${ext}",
      "target": "deb",
      "icon": "icon.png",
      "executableName": "parser2gis",
      "desktop": {
        "Name": "Parser2Gis",
        "Icon": "icon.png",
        "Comment": "2Gis parser",
        "Categories": "Network"
      }
    },
    "mac": {
      "target": "dmg"
    },
    "nsis": {
      "artifactName": "parser2gis${version}.${ext}"
    },
    "dmg": {
      "artifactName": "parser2gis${version}.${ext}"
    },
    "publish": {
      "provider": "generic",
      "url": "https://parselab.org/dist",
      "channel": "latest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
  "author": {
    "name": "ParseLab",
    "email": "info@parselab.org",
    "url": "https://parselab.org"
  },
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme",
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^22.7.0"
  },
  "dependencies": {
    "bluebird": "*",
    "electron-context-menu": "^0.13.0",
    "electron-is-dev": "*",
    "electron-updater": "^4.*",
    "iconv-lite": "*",
    "node-fetch": "*",
    "webix": "^3.4.*"
  }
}
